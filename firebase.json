{
  "hosting": {
    "public": "out",
    "target": "cabana-labs",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/auth/**",
        "function": "auth"
      },
      {
        "source": "/badges/**",
        "function": "badges"
      },
      {
        "source": "/did/**",
        "function": "did"
      },
      {
        "source": "/info/**",
        "function": "info"
      },
      {
        "source": "/status/**",
        "function": "status"
      },
      {
        "source": "/storage/**",
        "function": "storage"
      },
      {
        "source": "/org/**",
        "function": "org"
      },
      {
        "source": "/.well-known/did.json",
        "destination": "/well-known/did.json"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}